    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "linebreak-style": 0,
        "require-jsdoc" : 0,
        "object-curly-spacing": ["error", "always"],
        "arrow-parens": ["error", "always"]
